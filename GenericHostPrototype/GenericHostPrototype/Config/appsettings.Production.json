{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning"
    }
  },
  "SystemHealth": {
    "MaxMemoryThreshold": 1073741824,
    "MaxUptimeMinutes": 43200
  },
  "HostOptions": {
    "ShutdownTimeout": "00:00:15",
    "BackgroundServiceExceptionBehavior": "StopHost"
  }
}